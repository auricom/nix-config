{
  "extends": [
    "config:recommended",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":enablePreCommit",
    "github>auricom/nix-config//.github/renovate/autoMerge.json5",
    "github>auricom/nix-config//.github/renovate/commitMessage.json5",
    "github>auricom/nix-config//.github/renovate/labels.json5"
  ],
  "platform": "github",
  "username": "feisar-bot[bot]",
  "repositories": ["auricom/nix-config"],
  "assignees": ["auricom"],
  "onboarding": false,
  "requireConfig": "optional",
  "gitAuthor": "feisar-bot <feisar-bot[bot]@users.noreply.github.com>",
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": ["prIgnoreNotification"],
  "rebaseWhen": "conflicted",
  "commitBodyTable": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["(^|/)home/.+\\.nix$"],
      "matchStrings": [
        "refs/tags/(?<currentValue>.*)\"; # renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?) versioning=(?<versioning>.*?)"
      ]
    },
    {
      "customType": "regex",
      "fileMatch": ["(^|/)home/.+\\.nix$"],
      "matchStrings": ["plugin-kubectl-rev = \"(?<currentDigest>.*?)\";"],
      "currentValueTemplate": "main",
      "depNameTemplate": "evanlucas/fish-kubectl-completions",
      "packageNameTemplate": "https://github.com/evanlucas/fish-kubectl-completions",
      "datasourceTemplate": "git-refs"
    }
  ]
}